<p>Index Page</p>

<form action="/products" method="post">
Name: <input type="text" name="name"><br>
Price: <input type="text" name="price"><br>
Inventory <input type="text" name="inventory"><br>
<input type="submit" value="Submit">
</form>

{{#each products}}
<div class="products">

<h1>Name:{{name}}</h1>
<p>ID:{{id}}</p>
<p>Price:{{price}}</p>
<p>Inventory:{{inventory}}</p>
</div>

{{/each}}
